<?xml version="1.0" encoding="utf-8"?>
<Report
	ID="Dashboard"
	>
	<MasterReport
		Report="MasterReport"
	/>
	<StyleSheet
		StyleSheet="css.dashboard.css"
	/>
	<Body>
		<Remark>
			<ResponsiveRow
				CollisionBehavior="Overlap"
				>
				<ResponsiveColumn>
					<Division
						Class="card border border-dark"
						HtmlDiv="True"
						>
						<Division
							Class="card-header"
							HtmlDiv="True"
							>
							<Label
								Caption="Workflow Status Metrics"
								HtmlTag="Strong"
							/>
						</Division>
						<Division
							Class="card-body"
							HtmlDiv="True"
							>
							<ChartCanvas
								ChartOrientation="SwapAxes"
								ID="barWorkflowStatusMetrics"
								rdAutoSortingSeries="True"
								>
								<Series
									BarStacking="Stacked"
									ChartYDataColumn="rdCrosstabValue"
									Color="#4373AD,#c0504d,#9bbb59,#8064a2"
									LinkedToYAxisID="y-Axix"
									PointPadding="0"
									Type="Bar"
									>
									<DataLabels
										AlignmentHorizontal="Center"
										AlignmentVertical="Middle"
										Format="###,###"
									/>
									<Action
										Type="Report"
										>
										<Target
											FrameID="Self"
											Report="DashboardDrill.StatusMetrics"
											Type="Report"
										/>
										<LinkParams
											Category="@Chart.colCategoryFilter~"
											WORKFLOW_DESCRIPTION="@Chart.Workflow_Description~"
										/>
									</Action>
								</Series>
								<DataLayer
									ConnectionID="con_DemoAnalytics"
									ID="dlWorkflowStatusMetrics"
									Source="SELECT Workflow_Description
      ,sum(No_Of_Days) as &apos;SumNumberOfDays&apos;
	  , count(1) as &apos;COUNT&apos;
	  , BUSINESS_UNIT_NAME
  FROM Status_Metrics
  Group By Workflow_Description, BUSINESS_UNIT_NAME"
									Type="SQL"
									>
									<CalculatedColumn
										Formula="if (@Data.SumNumberOfDays~ &lt; 30)
	&apos;0-30&apos;;
else if (@Data.SumNumberOfDays~ &gt; 31 &amp;&amp; @Data.SumNumberOfDays~ &lt;= 50)
	&apos;31-50&apos;;
else if (@Data.SumNumberOfDays~ &gt; 51 &amp;&amp; @Data.SumNumberOfDays~ &lt;= 95)
	&apos;51-95&apos;;
else 
	&apos;&gt;95&apos;;"
										ID="colCategory"
									/>
									<CalculatedColumn
										Formula="if (@Data.SumNumberOfDays~ &lt; 30)
	1;
else if (@Data.SumNumberOfDays~ &gt; 31 &amp;&amp; @Data.SumNumberOfDays~ &lt;= 50)
	2;
else if (@Data.SumNumberOfDays~ &gt; 51 &amp;&amp; @Data.SumNumberOfDays~ &lt;= 95)
	3;
else 
	4;"
										ID="colSortBy"
									/>
									<SortFilter
										SortColumn="colSortBy,Workflow_Description"
									/>
									<CrosstabFilter
										CrosstabColumn="colCategory"
										CrosstabLabelColumn="Workflow_Description"
										CrosstabValueColumn="COUNT"
										CrosstabValueFunction="Sum"
										>
										<ExtraCrosstabLabelColumn
											CrosstabLabelColumn="colSortBy"
											ID="colCategoryFilter"
										/>
									</CrosstabFilter>
								</DataLayer>
								<ChartCanvasLegend
									AlignmentHorizontal="Right"
									AlignmentVertical="Middle"
									LegendOrientation="Vertical"
									LegendSpacing=" "
									ReversedItemOrder="True"
									ShowAllThreshold="99999"
								/>
								<ChartYAxis
									ID="y-Axix"
									>
									<AxisLabelStyle
										FontAngle="45"
										FontSize="10"
									/>
								</ChartYAxis>
								<ChartXAxis>
									<AxisLabelStyle
										FontSize="10"
									/>
								</ChartXAxis>
							</ChartCanvas>
						</Division>
					</Division>
				</ResponsiveColumn>
				<ResponsiveColumn>
					<Division
						Class="card border border-dark"
						HtmlDiv="True"
						>
						<Division
							Class="card-header"
							HtmlDiv="True"
							>
							<Label
								Caption="Error Metrics"
								HtmlTag="Strong"
							/>
						</Division>
						<Division
							Class="card-body"
							HtmlDiv="True"
							>
							<ChartCanvas
								ID="barErrorMetrics"
								rdAutoSortingSeries="True"
								>
								<Series
									BarStacking="Stacked"
									ChartXDataColumn="BUSINESS_UNIT_NAME"
									ChartXDataColumnType="Text"
									ChartYDataColumn="No_Of_Errors"
									Color="#4373AD"
									LinkedToYAxisID="y-Axix"
									PointPadding="0"
									Type="Bar"
									>
									<DataLabels
										AlignmentHorizontal="Center"
										AlignmentVertical="Bottom"
										Format="###,###"
										OffsetY="-20"
									/>
									<Action
										Type="Report"
										>
										<Target
											FrameID="Self"
											Report="DashboardDrill.ErrorMetrics"
											Type="Report"
										/>
										<LinkParams
											BUSINESS_UNIT="@Chart.BUSINESS_UNIT~"
										/>
									</Action>
								</Series>
								<DataLayer
									ConnectionID="con_DemoAnalytics"
									ID="dlErrorMetrics"
									Source="SELECT BUSINESS_UNIT
      ,BUSINESS_UNIT_NAME
      /*,Unit_Number
      ,Lease_Number*/
      /*,Processed_By
      ,Processed_Date*/
      ,sum(No_Of_Transactions) as No_Of_Transactions
      ,sum(No_Of_Errors) as No_Of_Errors
  FROM Error_Metrics
  group by  Business_Unit
      ,BUSINESS_UNIT_NAME
order by BUSINESS_UNIT_NAME"
									Type="SQL"
								/>
								<ChartCanvasLegend
									AlignmentHorizontal="Right"
									AlignmentVertical="Middle"
									LegendOrientation="Vertical"
									LegendSpacing=" "
									ShowAllCaption=" "
								/>
								<ChartYAxis
									ID="y-Axix"
									>
									<AxisLabelStyle
										FontAngle="45"
										FontSize="10"
									/>
								</ChartYAxis>
								<ChartXAxis>
									<AxisLabelStyle
										FontAngle="45"
										FontSize="10"
									/>
								</ChartXAxis>
								<Series
									ChartXDataColumn="BUSINESS_UNIT_NAME"
									ChartXDataColumnType="Text"
									ChartYDataColumn="No_Of_Transactions"
									Color="#c0504d"
									Type="Spline"
									>
									<DataLabels
										AlignmentHorizontal="Center"
										AlignmentVertical="Bottom"
										Format="###,###"
										OffsetY="-20"
									/>
									<MarkerPoints/>
								</Series>
							</ChartCanvas>
						</Division>
					</Division>
				</ResponsiveColumn>
			</ResponsiveRow>
		</Remark>
		<Remark>
			<ResponsiveRow
				CollisionBehavior="Overlap"
				>
				<ResponsiveColumn>
					<Division
						Class="card border border-dark"
						HtmlDiv="True"
						>
						<Division
							Class="card-header"
							HtmlDiv="True"
							>
							<Label
								Caption="Lease Expiration Metrics"
								HtmlTag="Strong"
							/>
						</Division>
						<Division
							Class="card-body p-0 bg-gradent-light"
							HtmlDiv="True"
							>
							<ChartCanvas
								ID="barLeaseExpirationMetrics"
								rdAutoSortingSeries="True"
								>
								<Series
									ChartXDataColumn="colCategory"
									ChartYDataColumn="aggColCntExires"
									Color="#4373AD"
									LinkedToYAxisID="y-Axix"
									Type="Bar"
									>
									<DataLabels
										AlignmentHorizontal="Center"
										AlignmentVertical="Bottom"
										FontColor="#ffffff"
										FontSize="10"
									/>
									<Action
										Type="Report"
										>
										<Target
											FrameID="Self"
											Report="DashboardDrill.LeaseExpireMetrics"
											Type="Report"
										/>
										<LinkParams
											EndDate="@Chart.colExpiresIn~"
										/>
									</Action>
								</Series>
								<DataLayer
									ConnectionID="con_DemoAnalytics"
									ID="dlLeaseExpirationMetrics"
									Source="SELECT count(1) as cntRows
	, END_DATE
  FROM Lease_Expiration_Metrics
  group by END_DATE
UNION 
	SELECT count(1) as cntRows 
	, CONVERT(DATETIME,&apos;01/01/2050&apos;) as &apos;END_DATE&apos;
  FROM Lease_Expiration_Metrics
ORDER BY END_DATE"
									Type="SQL"
									>
									<TimePeriodColumn
										DataColumn="END_DATE"
										ID="colExpiresIn"
										TimePeriod="FirstDayOfMonth"
									/>
									<CalculatedColumn
										Formula="if (Year(&apos;@Data.colExpiresIn~&apos;) &lt; 2030)
	MonthName(Month(&apos;@Data.colExpiresIn~&apos;), true) + &apos;, &apos; + Year(&apos;@Data.colExpiresIn~&apos;);
else 
	&apos;Total&apos;;"
										ID="colCategory"
									/>
									<GroupFilter
										GroupColumn="colCategory"
										ID="grpLeaseExpiration"
										>
										<GroupAggregateColumn
											AggregateColumn="cntRows"
											AggregateFunction="Sum"
											ID="aggColCntExires"
										/>
									</GroupFilter>
								</DataLayer>
								<ChartCanvasLegend
									AlignmentHorizontal="Right"
									AlignmentVertical="Middle"
									LegendOrientation="Vertical"
									LegendSpacing=" "
									ShowAllCaption=" "
								/>
								<ChartYAxis
									HideAxis="True"
									ID="y-Axix"
									>
									<AxisLabelStyle
										FontAngle="45"
										FontSize="10"
									/>
								</ChartYAxis>
								<ChartXAxis
									AxisLineColorTransparency="15"
									>
									<AxisLabelStyle
										FontAngle="45"
										FontSize="10"
									/>
									<ChartTicksAndGrid
										AlternateGridColor="Transparent"
										AlternateGridColorTransparency="15"
										GridLineColor="Transparent"
										GridLineColorTransparency="15"
										GridLineThickness="0"
										TickLineColor="Transparent"
										TickLineColorTransparency="15"
										TickLineLength="0"
										TickLineThickness="0"
									/>
								</ChartXAxis>
							</ChartCanvas>
							<Remark>
								<ChartCanvas
									ID="barLeaseExpirationMetrics"
									rdAutoSortingSeries="True"
									>
									<Series
										ChartXDataColumn="XAxis"
										ChartYDataColumn="YAxis"
										Color="#4f81bd"
										LinkedToYAxisID="y-Axix"
										Type="Bar"
										>
										<DataLabels
											AlignmentHorizontal="Center"
											AlignmentVertical="Bottom"
										/>
										<Action
											Type="Report"
											>
											<Target
												FrameID="Self"
												Report="DashboardDrill.LeaseExpireMetrics"
												Type="Report"
											/>
											<LinkParams
												EndDate="@Chart.colExpiresIn~"
											/>
										</Action>
									</Series>
									<DataLayer
										ConnectionID="con_DemoAnalytics"
										ID="dlLeaseExpirationMetrics"
										Source="SELECT count(1) as &apos;YAxis&apos;
      , convert(char(3), END_DATE, 0) + &apos;, &apos; + datename(year, END_DATE) as &apos;XAxis&apos;
      , DATEADD(month, DATEDIFF(month, 0, END_DATE), 0) as sortBy
  FROM Lease_Expiration_Metrics
  GROUP BY convert(char(3), END_DATE, 0), datename(year, END_DATE), DATEADD(month, DATEDIFF(month, 0, END_DATE), 0) 
UNION 
	SELECT count(1) as &apos;YAxis&apos;
      , &apos;TOTAL&apos; as &apos;XAxis&apos;
	  , DATEADD(year, 10, getdate()) as sortBy
  FROM Lease_Expiration_Metrics
order by sortBy"
										Type="SQL"
										>
										<Remark>
											<TimePeriodColumn
												DataColumn="END_DATE"
												ID="colExpiresIn"
												TimePeriod="FirstDayOfMonth"
											/>
										</Remark>
										<Remark>
											<TimePeriodColumn
												ID="colExpiresSort"
												TimePeriod="Date"
											/>
										</Remark>
										<Remark>
											<AggregateColumn
												AggregateColumn="BUSINESS_UNIT"
												AggregateFunction="Count"
												ID="total"
											/>
										</Remark>
										<Remark>
											<GroupFilter
												GroupColumn="colExpiresIn"
												ID="grpLeaseExpiration"
												>
												<GroupAggregateColumn
													AggregateColumn="colExpiresIn"
													AggregateFunction="Count"
													ID="aggColCntExires"
												/>
											</GroupFilter>
										</Remark>
										<Join
											ID="unionTotal"
											JoinType="Union"
											>
											<DataLayer
												ConnectionID="con_DemoAnalytics"
												Source="SELECT count(1) as &apos;YAxis&apos;
      , &apos;TOTAL&apos; as &apos;XAxis&apos;
  FROM Lease_Expiration_Metrics"
												Type="SQL"
											/>
										</Join>
									</DataLayer>
									<ChartCanvasLegend
										AlignmentHorizontal="Right"
										AlignmentVertical="Middle"
										LegendOrientation="Vertical"
										LegendSpacing=" "
										ShowAllCaption=" "
									/>
									<ChartYAxis
										ID="y-Axix"
									/>
									<ChartXAxis>
										<AxisLabelStyle
											FontAngle="45"
											FontSize="10"
										/>
									</ChartXAxis>
								</ChartCanvas>
							</Remark>
						</Division>
					</Division>
				</ResponsiveColumn>
				<ResponsiveColumn>
					<Division
						Class="card border border-dark"
						HtmlDiv="True"
						>
						<Division
							Class="card-header"
							HtmlDiv="True"
							>
							<Label
								Caption="Vacant %"
								HtmlTag="Strong"
							/>
						</Division>
						<Division
							Class="card-body p-0 bg-gradent-light"
							HtmlDiv="True"
							>
							<ChartCanvas
								ID="barVacantPercent"
								rdAutoSortingSeries="True"
								SpacingLeft="20"
								>
								<Series
									BarStacking="Stacked"
									ChartXDataColumn="BUSINESS_UNIT_NAME"
									ChartXDataColumnType="Text"
									ChartYDataColumn="colPercentVacant"
									Color="#4373AD"
									LinkedToYAxisID="y-Axix"
									PointPadding="0"
									Type="Bar"
									>
									<DataLabels
										AlignmentHorizontal="Center"
										AlignmentVertical="Bottom"
										FontColor="#ffffff"
										Format="##\%"
									/>
									<Action
										Type="Report"
										>
										<Target
											FrameID="Self"
											Report="DashboardDrill.OccupancyMetrics"
											Type="Report"
										/>
										<LinkParams
											BUSINESS_UNIT="@Chart.BUSINESS_UNIT~"
										/>
									</Action>
								</Series>
								<DataLayer
									ConnectionID="con_DemoAnalytics"
									ID="dlVacantPercent"
									Source="SELECT BUSINESS_UNIT
      ,BUSINESS_UNIT_NAME
      /*,UNIT_NUMBER*/
	  , count(1) as cntOccupied
      /*,UNIT_TYPE
      ,SQUARE_FEET*/
      ,TOTAL_AREA
      ,TOTAL_UNITS
  FROM Occupancy_Metrics
  GROUP BY BUSINESS_UNIT
	,BUSINESS_UNIT_NAME
	,TOTAL_AREA
	,TOTAL_UNITS
ORDER BY BUSINESS_UNIT_NAME"
									Type="SQL"
									>
									<CalculatedColumn
										Formula="(@Data.cntOccupied~/@Data.TOTAL_UNITS~)*100"
										ID="colPercentVacant"
									/>
								</DataLayer>
								<ChartCanvasLegend
									AlignmentHorizontal="Right"
									AlignmentVertical="Middle"
									LegendOrientation="Vertical"
									LegendSpacing=" "
									ShowAllCaption=" "
								/>
								<ChartYAxis
									HideAxis="True"
									ID="y-Axix"
								/>
								<ChartXAxis>
									<AxisLabelStyle
										FontAngle="45"
										FontSize="10"
									/>
								</ChartXAxis>
							</ChartCanvas>
						</Division>
					</Division>
				</ResponsiveColumn>
			</ResponsiveRow>
		</Remark>
		<Division
			Class="dashboard-responsive"
			HtmlDiv="True"
			ID="divDashboard"
			>
			<Dashboard2
				AllowFreeformLayout="False"
				DashboardColumns="2"
				ID="VisualizationsDashboard"
				SaveFile="@Function.AppPhysicalPath~\SavedDashboards\Visualizations_User_@Function.UserName~.xml"
				TemplateModifierFile="DashDMF.xml"
				>
				<Panel
					ID="pnlReconcilation"
					>
					<PanelContent>
						<Remark>
							<DataTable
								AccessibleHeaders="True"
								AccessibleSummary="DataTable"
								ID="dtReconcilation"
								KeepScrollPosition="True"
								RememberSort="True"
								ResizableColumns="True"
								SortArrows="True"
								TableBorder="0"
								>
								<DataLayer
									ConnectionID="con_DemoAnalytics"
									ID="dlReconcilation"
									Source="select * from reconcilation"
									Type="SQL"
								/>
								<InteractivePaging
									CaptionType="Image"
									HideShowPrevNextCaptions="False"
									HideWhenOnePage="True"
									IDnotSupportedHere="pagingTableNumbered"
									Location="Top"
									PageRowCount="20"
									ShowPageNumber="Numbered"
								/>
								<DataTableColumn
									Header="Bucket"
									ID="colBucket"
									>
									<Label
										Caption="@Data.Bucket~"
										ID="lblBucket"
										rdForWizardSet="true"
									/>
									<DataColumnSort
										DataColumn="Bucket"
										DataType="Text"
									/>
								</DataTableColumn>
								<DataTableColumn
									Header="Year"
									ID="colYear"
									>
									<Label
										Caption="@Data.Year~"
										ID="lblYear"
										rdForWizardSet="true"
									/>
									<DataColumnSort
										DataColumn="Year"
										DataType="Text"
									/>
								</DataTableColumn>
								<DataTableColumn
									Header="Lease Counts"
									ID="colLeaseCount"
									>
									<Remark>
										<Gauge
											AnimationDuration="0"
											BulletColor="#85D0F5"
											ChartHeight="70"
											ChartWidth="100%"
											Tooltip="@Data.Property_Count~"
											Type="BulletBar"
											Value="@Data.Property_Count~"
											>
											<GaugeRange
												Color="#b84026"
												RangeEndValue="30"
											/>
											<GaugeRange
												Color="#dcd440"
												RangeEndValue="60"
											/>
											<GaugeRange
												Color="#85b74d"
												RangeEndValue="100"
											/>
										</Gauge>
									</Remark>
									<CellBar
										Color="#799FCD"
										DataColumn="Property_Count"
										ID="pp"
										Tooltip="@Data.Property_Count~"
									/>
									<Label
										Caption="@Data.Property_Count~"
										ID="lbl"
									/>
								</DataTableColumn>
								<DataTableColumn
									Header="Amount"
									ID="colAmount"
									>
									<DataColumnSort
										DataColumn="Amount"
										DataType="Text"
									/>
									<CellBar
										Color="#799FCD"
										DataColumn="Amount"
										ID="pp"
										Tooltip="@Data.Amount~"
									/>
									<Label
										Caption="@Data.Amount~"
										ID="lblAmount"
										rdForWizardSet="true"
									/>
								</DataTableColumn>
							</DataTable>
						</Remark>
						<Remark>
							<DataTable
								AccessibleHeaders="True"
								AccessibleSummary="DataTable"
								ID="dtCount"
								KeepScrollPosition="True"
								RememberSort="True"
								ResizableColumns="True"
								SortArrows="True"
								TableBorder="0"
								>
								<DataLayer
									ColumnNamesRow="1"
									ExcelFile="D:\ReconcilationRowDatas.xls"
									ExcelWorksheet="Count"
									ID="ExcelDataLayer1"
									Type="Excel"
								/>
								<InteractivePaging
									CaptionType="Image"
									HideShowPrevNextCaptions="False"
									HideWhenOnePage="True"
									IDnotSupportedHere="pagingTableNumbered"
									Location="Top"
									PageRowCount="20"
									ShowPageNumber="Numbered"
								/>
								<DataTableColumn
									Header="Bucket"
									ID="colBucket"
									>
									<Label
										Caption="@Data.Bucket~"
										ID="lblBucket"
										rdForWizardSet="true"
									/>
									<DataColumnSort
										DataColumn="Bucket"
										DataType="Date"
									/>
								</DataTableColumn>
								<DataTableColumn
									Header="2015"
									ID="col2015"
									>
									<CellBar
										Color="#799FCD"
										DataColumn="2015"
										ID="pp"
										Tooltip="@Data.2015~"
									/>
									<Spaces/>
									<Label
										Caption="@Data.2015~"
										ID="lbl2015"
										rdForWizardSet="true"
									/>
									<DataColumnSort
										DataColumn="2015"
										DataType="Number"
									/>
								</DataTableColumn>
								<DataTableColumn
									Header="2014"
									ID="col2014"
									>
									<CellBar
										Color="#799FCD"
										DataColumn="2014"
										ID="pp"
										Tooltip="@Data.2014~"
									/>
									<Spaces/>
									<Label
										Caption="@Data.2014~"
										ID="lbl2014"
										rdForWizardSet="true"
									/>
									<DataColumnSort
										DataColumn="2014"
										DataType="Number"
									/>
								</DataTableColumn>
								<DataTableColumn
									Header="2013"
									ID="col2013"
									>
									<CellBar
										Color="#799FCD"
										DataColumn="2013"
										ID="pp"
										Tooltip="@Data.2013~"
									/>
									<Spaces/>
									<Label
										Caption="@Data.2013~"
										ID="lbl2013"
										rdForWizardSet="true"
									/>
									<DataColumnSort
										DataColumn="2013"
										DataType="Number"
									/>
								</DataTableColumn>
								<DataTableColumn
									Header="2012"
									ID="col2012"
									>
									<CellBar
										Color="#799FCD"
										DataColumn="2012"
										ID="pp"
										Tooltip="@Data.2012~"
									/>
									<Spaces/>
									<Label
										Caption="@Data.2012~"
										ID="lbl2012"
