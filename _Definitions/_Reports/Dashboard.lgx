					ID="pnlWorkflowStatusMetrics"
					Image="dash.Workflow.png"
					>
					<PanelContent>
						<ChartCanvas
							ChartOrientation="SwapAxes"
							ID="barWorkflowStatusMetrics"
							rdAutoSortingSeries="True"
							>
							<Series
								BarStacking="Stacked"
								ChartYDataColumn="rdCrosstabValue"
								Color="#4373AD,#c0504d,#9bbb59,#8064a2"
								LinkedToYAxisID="y-Axix"
								PointPadding="0"
								Type="Bar"
								>
								<Quicktip
									ID="qtWorkFlow"
									Title="@Chart.Workflow_Description~"
									>
									<Remark>
										<QuicktipRow
											Caption="Workflow"
											Value="@Chart.Workflow_Description~"
										/>
									</Remark>
									<QuicktipRow
										Caption="Category"
										Value="@Chart.rdCrosstabColumn~"
									/>
									<QuicktipRow
										Caption="Count:"
										Value="@Chart.rdCrosstabValue~"
									/>
									<Remark>
										<QuicktipRow
											Caption="# of Days:"
											Value="@Chart.colCategoryFilter~"
										/>
									</Remark>
								</Quicktip>
								<DataLabels
									AlignmentHorizontal="Center"
									AlignmentVertical="Middle"
									FontColor="#ffffff"
									FontSize="10px"
									Format="###,###"
								/>
								<Action
									Type="Report"
									>
									<Target
										FrameID="Self"
										Report="DashboardDrill.StatusMetrics"
										Type="Report"
									/>
									<LinkParams
										WORKFLOW_DESCRIPTION="@Chart.Workflow_Description~"
									/>
									<Remark>
										<LinkParams
											Category="@Chart.colCategoryFilter~"
											WORKFLOW_DESCRIPTION="@Chart.Workflow_Description~"
										/>
									</Remark>
								</Action>
							</Series>
							<DataLayer
								ConnectionID="con_DemoAnalytics"
								ID="dlWorkflowStatusMetrics"
								Source="SELECT ID,SM.Workflow_Description
      ,sum(No_Of_Days) as &apos;SumNumberOfDays&apos;
	  , count(1) as &apos;COUNT&apos;
	  , BUSINESS_UNIT_NAME
  FROM Status_Metrics SM
  inner join Status_Metrics_Sort S on S.Workflow_Description=SM.Workflow_Description

  Group By SM.Workflow_Description, BUSINESS_UNIT_NAME,ID
    order by ID desc"
								Type="SQL"
								>
								<CalculatedColumn
									Formula="if (@Data.SumNumberOfDays~ &lt; 30)
	&apos;0-30&apos;;
else if (@Data.SumNumberOfDays~ &gt; 31 &amp;&amp; @Data.SumNumberOfDays~ &lt;= 50)
	&apos;31-50&apos;;
else if (@Data.SumNumberOfDays~ &gt; 51 &amp;&amp; @Data.SumNumberOfDays~ &lt;= 95)
